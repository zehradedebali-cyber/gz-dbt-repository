select o.orders_id,
       o.date_date, 
       round((o.margin+s.shipping_fee-s.logcost-s.ship_cost), 2) as operational_margin,
       o.revenue,
       o.quantity,
       o.purchase_cost,
       o.margin,
       s.shipping_fee,
       s.ship_cost,
       s.logcost
from {{(int_orders_operatinal)}} o
left join {{(stg_raw__ship)}} s 
using (orders_id)
group by orders_id, date_date
order by date_date desc;